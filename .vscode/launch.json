{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Run generate_rollouts.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/tests/generate_rollouts.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: Debug pytest",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "args": ["-q"]
        }
    ]
}